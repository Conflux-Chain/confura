{
    "endpoint": ":22537",
    "cfx": {
        "ws": "ws://test.confluxrpc.com/ws",
        "http": "http://test.confluxrpc.com",
        "retry": 3,
        "retryInterval": 1000
    },
    "sync": {
        "poll": {
            "interval": 300
        },
        "sub": {
            "buffer": 1000
        },
        "maxEpochs": 10
    },
    "nearhead": {
        "cap": 100000,
        "logLimit": 3000
    },
    "metrics": {
        "enabled": true,
        "influxdb": {
            "host": "http://127.0.0.1:8086",
            "db": "infura_test",
            "username": "",
            "password": ""
        },
        "report": {
            "enabled": false,
            "interval": 10000
        }
    },
    "log": {
        "level": "info"
    },
    "store": {
        "mysql": {
            "enabled": false,
            "username": "",
            "password": "",
            "database": "conflux_infura",
            "connMaxLifeTime": "3m",
            "maxOpenConns": 10,
            "maxIdleConns": 10
        },
        "redis": {
            "enabled": false,
            "url": "redis://<user>:<pass>@localhost:6379/<db>"
        }
    },
    "alert": {
        "customTags": ["testnet", "prod"],
        "dingtalk": {
            "enabled": false,
            "webhook": "can also be defined with INFURA_ALERT_DINGTALK_WEBHOOK enviroment variable",
            "secret":  "can also be defined with INFURA_ALERT_DINGTALK_SECRET enviroment variable",
            "atMobiles": [],
            "isAtAll": false
        }
    },
    "prune": {
        "db": {
            "interval": 30000,
            "maxEpochs": 10,
            "threshold": {
                "maxBlocks": 20000000,
                "maxTxs": 20000000,
                "maxLogs": 20000000
            }
        }
    },
    "node": {
        "urls": [
            "http://test.confluxrpc.com",
            "http://test.confluxrpc.org/v2",
            "http://portal-test.confluxrpc.com"
        ],
        "hashRing": {
            "partitionCount": 15739,
            "replicationFactor": 51,
            "load": 1.25
        },
        "monitor": {
            "interval": "1s",
            "unhealth": {
                "failures": 3,
                "epochsFallBehind": 30,
                "latencyPercentile": 0.9,
                "maxLatency": "3s"
            },
            "recover": {
                "remindInterval": "5m",
                "successCounter": 60
            }
        },
        "endpoint": ":22530",
        "router": {
            "redisUrl": "",
            "nodeRpcUrl": ""
        }
    }
}